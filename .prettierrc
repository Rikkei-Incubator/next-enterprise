{
  "tabWidth": 2,
  "useTabs": false,
  "parser": "typescript",
  "printWidth": 120,
  "arrowParens": "always",
  "bracketSpacing": true,
  "proseWrap": "preserve",
  "semi": false,
  "singleQuote": true,
  "trailingComma": "all",
  "jsxSingleQuote": false,
  "tailwindConfig": "./tailwind.config.js",
  "plugins": [
    "prettier-plugin-tailwindcss"
  ]
}
